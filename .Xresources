!-- Xft settings -- !
Xft.dpi:		96
Xft.antialias:	true
Xft.rgba:		rgb
Xft.hinting:	true
Xft.autohint:	true
Xft.hintstyle:	hintfull
Xft.lcdfilter:  lcddefault

!URxvt.font:					xft:Inconsolata for Powerline:size=14:antialias=true:hinting=true:hintsyle=hintslight  
!URxvt.boldFont:             xft:Inconsolata for Powerline:size=14:antialias=true:hinting=true:hintsyle=hintslight  
!URxvt.italicFont:           xft:Inconsolata for Powerline:size=14:antialias=true:hinting=true:hintsyle=hintslight  
!URxvt.boldItalicFont:       xft:Inconsolata for Powerline:size=14:antialias=true:hinting=true:hintsyle=hintslight  

Xcursor.theme:              dmz-aa
! Cursor
URxvt.cursorBlink: 			true
!URxvt.cursorColor: 			#657b83
URxvt.cursorUnderline: 		false

URxvt.font:					xft:CosmicSansNeueMono:size=13:antialias=true:hinting=true:hintsyle=hintslight
URxvt.boldFont:				xft:CosmicSansNeueMono:bold:size=13:antialias=true:hinting=true:hintsyle=hintslight
URxvt.italicFont:			xft:CosmicSansNeueMono:size=13:antialias=true:hinting=true:hintsyle=hintslight
URxvt.boldItalicFont:		xft:CosmicSansNeueMono:size=13:antialias=true:hinting=true:hintsyle=hintslight

URxvt.modifier:             alt
urxvt.buffered:				true
urxvt.scrollBar:			false
URxvt.loginShell:			true
URxvt.internalBorder:		9
URxvt.iso14755:				true
URxvt.iso14755_52:			true
URxvt.mouseWheelScrollPage: true
URxvt.saveLines:            32767
URxvt.urgentOnBell:         true

urxvt.perl-ext:				default,matcher
urxvt.urlLauncher:			/usr/bin/firefox
urxvt.matcher.button:		1

!URxvt.url-select.launcher:  web_browser
!URxvt.perl-ext-common:      font-size,keyboard-select,url-select,clipboard,autocomplete-ALL-the-things

!*foreground:				#c5c8c6
!*background:				#1d1f21
!*cursorColor:				#aeafad
!*color0:					#000000
!*color1:					#912226
